def selection_sort(arr):
    n = len(arr)
    
    for i in range(n - 1):
        min_index = i  # Предполагаем, что минимальный элемент — текущий
        
        for j in range(i + 1, n):
            if arr[j] < arr[min_index]:
                min_index = j# Ищем минимальный элемент в неотсортированной части
        
        arr[i], arr[min_index] = arr[min_index], arr[i] # Меняем найденный минимум с текущим элементом
    
    return arr
